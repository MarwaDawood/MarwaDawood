  width: screen;
}

.bubbles span {
  position: relative;
  width: 10px;
  height: 10px;
  margin: 0 4px;
  border-radius: 50%;
  background-color: #F1C844;
  box-shadow: 0 0 0 4px #F1C84444,
  0 0 4px #F1C844,
  0 0 4px #F1C844;
  animation: animate 15s linear infinite;
  animation-duration: calc(200s / var(--i))

}


.bubbles span:nth-child(even)
{
  background-color: #9162B3;
  box-shadow: 0 0 4px #9162B344,
  0 0 4px #9162B3,
  0 0 4px #9162B3;
}



.bubbles span:nth-child(3n)
{
